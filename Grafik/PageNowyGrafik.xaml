      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Grafik"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PageNowyGrafik">

    <Grid>
        <ListBox x:Name="listBox"   HorizontalAlignment="Left" Height="100" Margin="25,30,0,0" VerticalAlignment="Top" Width="100"/>

        <ListBox x:Name="listBox1"  HorizontalAlignment="Left" Height="100" Margin="25,150,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="button" Click="button_Click" Content="Button" HorizontalAlignment="Left" Margin="170,230,0,0" VerticalAlignment="Top" Width="85" Height="30"/>

    </Grid>
</Page>
